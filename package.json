{
  "name": "cypress-circleci-reporter",
  "version": "0.3.0",
  "author": "Kamil Socha (https://github.com/ksocha)",
  "description": "Cypress test reporter for CircleCI",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/author/library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksocha/cypress-circleci-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/ksocha/cypress-circleci-reporter/issues"
  },
  "keywords": [
    "circleci",
    "reporter",
    "cypress",
    "parallelization",
    "testing"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest ./tests/ --globals",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/mocha": "^8.2.3",
    "@types/xml": "^1.0.11",
    "cypress": "^4.12.1",
    "mocha": "^8.4.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "strip-ansi": "^7.1.2",
    "xmlbuilder2": "^4.0.0"
  },
  "peerDependencies": {
    "mocha": ">=7"
  }
}
