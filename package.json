{
  "name": "cypress-circleci-reporter",
  "version": "0.4.0",
  "author": "Kamil Socha (https://github.com/ksocha)",
  "description": "Cypress test reporter for CircleCI",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/author/library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksocha/cypress-circleci-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/ksocha/cypress-circleci-reporter/issues"
  },
  "keywords": [
    "circleci",
    "reporter",
    "cypress",
    "parallelization",
    "testing"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run ./tests/ --globals",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/mocha": "^8.2.3",
    "@types/node": "^24.10.1",
    "@types/xml": "^1.0.11",
    "cypress": "^15.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "jest-date-mock": "^1.0.10",
    "jest-xml-matcher": "^1.2.0",
    "mocha": "^8.4.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "strip-ansi": "^7.1.2",
    "xmlbuilder2": "^4.0.0"
  },
  "peerDependencies": {
    "mocha": ">=7"
  }
}
